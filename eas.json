{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_PROJECT_ROOT": "."
      },
      "prebuildCommand": "bash ./prepare-build.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
